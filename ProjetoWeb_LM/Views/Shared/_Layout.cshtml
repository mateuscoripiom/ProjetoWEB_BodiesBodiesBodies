<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color: #1f1f1d">
        <div class="container">

            <div class="navbar-collapse collapse">


                <div class="1">
                    <ul class="nav navbar-nav">
                        <li><img src="~/imagens/logo.png" width="10%" /></li>
                        <li class="cor1">@Html.ActionLink("INÍCIO", "Index", "Home", new { @id = "cor" })</li>
                        <li class="cor2">@Html.ActionLink("CADASTRO", "Usuario", "Home", new { @id = "cor1" })</li>
                        <li class="cor3">@Html.ActionLink("RESULTADO", "Resultado", "Home", new { @id = "cor2" })</li>
                    </ul>
                </div>
            </div>

        </div>
    </div>
    </div>
    <div class="container body-content">
        @RenderBody()

        <footer>
            <p class="baixo">&copy; @DateTime.Now.Year - BODIES BODIES BODIES </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
